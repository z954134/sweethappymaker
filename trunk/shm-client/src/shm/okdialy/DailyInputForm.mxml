<?xml version="1.0" encoding="utf-8"?>
<mx:HBox xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%"
		 xmlns:cmn="shm.common.*">

	<cmn:DateColumnFormatter id="dateColumnFormatter"/>
	<mx:Form>
		<mx:FormHeading label="10このOK"/>
		<mx:FormItem label="日付">
			<mx:DateField id="okDialyDate" />
		</mx:FormItem>
		<mx:FormItem label="1">
			<mx:TextInput id="ok1"/>
		</mx:FormItem>
		<mx:FormItem label="2">
			<mx:TextInput id="ok2"/>
		</mx:FormItem>
		<mx:FormItem label="3">
			<mx:TextInput id="ok3"/>
		</mx:FormItem>
		<mx:FormItem label="4">
			<mx:TextInput id="ok4"/>
		</mx:FormItem>
		<mx:FormItem label="5">
			<mx:TextInput id="ok5"/>
		</mx:FormItem>
		<mx:FormItem label="6">
			<mx:TextInput id="ok6"/>
		</mx:FormItem>
		<mx:FormItem label="7">
			<mx:TextInput id="ok7"/>
		</mx:FormItem>
		<mx:FormItem label="8">
			<mx:TextInput id="ok8"/>
		</mx:FormItem>
		<mx:FormItem label="9">
			<mx:TextInput id="ok9"/>
		</mx:FormItem>
		<mx:FormItem label="10">
			<mx:TextInput id="ok10"/>
		</mx:FormItem>
		<mx:FormItem>
			<mx:Button label="登録" id="submitButton" click="insertService.send()"/>
			<mx:Button id="hintButton" label="ヒント"/>
		</mx:FormItem>

	</mx:Form>
	<!--	<mx:VBox>
		 <mx:Spacer height="10"/>
		 <mx:DateChooser id="okDialyDate" showToday="true"/>
		 </mx:VBox>
	-->
	<mx:HTTPService id="insertService" url="/okdialy/insert" method="post" showBusyCursor="true">
		<mx:request>
			<okDialyDate>{okDialyDate.text}</okDialyDate>
			<item_1>{ok1.text}</item_1>
			<item_2>{ok2.text}</item_2>
			<item_3>{ok3.text}</item_3>
			<item_4>{ok4.text}</item_4>
			<item_5>{ok5.text}</item_5>
			<item_6>{ok6.text}</item_6>
			<item_7>{ok7.text}</item_7>
			<item_8>{ok8.text}</item_8>
			<item_9>{ok9.text}</item_9>
			<item_10>{ok10.text}</item_10>
		</mx:request>
	</mx:HTTPService>
</mx:HBox>
